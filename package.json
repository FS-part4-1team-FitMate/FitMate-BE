{
  "name": "fitmate-be",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "main": "dist/src/main.js",
  "type": "module",
  "imports": {
    "#auth/*": "./dist/src/auth/*",
    "#common/*": "./dist/src/common/*",
    "#exception/*": "./dist/src/exception/*",
    "#user/*": "./dist/src/user/*",
    "#profile/*": "./dist/src/profile/*",
    "#lesson/*": "./dist/src/lesson/*",
    "#direct-lesson/*": "./dist/src/direct-lesson/*",
    "#trainer/*": "./dist/src/trainer/*",
    "#quote/*": "./dist/src/quote/*",
    "#review/*": "./dist/src/review/*",
    "#notification/*": "./dist/src/notification/*",
    "#configs/*": "./dist/src/configs/*",
    "#s3/*": "./dist/src/s3/*",
    "#utils/*": "./dist/src/utils/*",
    "#logger/*": "./dist/src/logger/*",
    "#prisma/*": "./dist/prisma/*"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.726.1",
    "@aws-sdk/s3-request-presigner": "^3.726.1",
    "@nestjs/axios": "^3.1.3",
